                android:background=""
                android:gravity="top"
                android:maxLines="50"
                android:padding="20dp"
                android:singleLine="false"
                android:text="07:00 \n15:00"
                android:textColor="@color/history_clock_time" />



            <TextView
                android:id="@+id/shift_history2"
